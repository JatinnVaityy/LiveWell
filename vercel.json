{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)","destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "status": 204
    }
  ]
}
